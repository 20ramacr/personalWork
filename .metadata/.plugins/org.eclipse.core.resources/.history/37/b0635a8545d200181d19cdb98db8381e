int one= 1;
int two = 2;
int three = 3;

if (one>two && one>three)
{
	System.out.println(one);
	
}
if (two>one && two>three)
{
System.out.println(two);
}
if (three>one && three>two)
{
	System.out.println(three);
	}
	}
